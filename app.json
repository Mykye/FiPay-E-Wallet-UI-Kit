{
  "expo": {
    "name": "RNFiPay_E-Wallet_Ui_Kit",
    "slug": "RNFiPay_E-Wallet_Ui_Kit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/imageForScreen.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/imageForScreen.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/imageForScreen.jpg",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/imageForScreen.jpg"
    },
    "plugins": [
      [
        "expo-notifications"
      ]
    ],
    "description": ""
  }
}
